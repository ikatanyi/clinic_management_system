	<queryString>
		<![CDATA[SELECT
     ac_cash_collection.`activity_code` AS activity_code,
     ac_cash_collection.`patient_no` AS patient_no,
     ac_cash_collection.`payment_mode` AS payment_mode,
     ac_cash_collection.`id` AS id,
     ac_cash_collection.`account_no` AS account_no,
     ac_cash_collection.`debit` AS debit,
     ac_cash_collection.`credit` AS credit,
     ac_cash_collection.`date` AS date,
     ac_cash_collection.`transaction_no` AS transaction_no,
     ac_cash_collection.`drawer_bank` AS drawer_bank,
     CONCAT(dt_patientdetails.`surname`," ",dt_patientdetails.`Fname`," ",dt_patientdetails.`Lname`) AS patientname,
     ac_cash_collection.`cash_point` AS cash_point
FROM
     `ac_cash_collection` ac_cash_collection LEFT JOIN `dt_patientdetails` dt_patientdetails ON ac_cash_collection.`patient_no` = dt_patientdetails.`patient_id`
where credit=0 AND ($P{deptCode} ='' OR ac_cash_collection.`cash_point`=$P{deptCode}) AND ac_cash_collection.`date` BETWEEN $P{from} AND $P{to}
GROUP BY ac_cash_collection.`cash_point`,ac_cash_collection.`payment_mode`, ac_cash_collection.`id`
order by ac_cash_collection.`cash_point` ASC]]>
	</queryString>
