	<queryString>
		<![CDATA[SELECT
     ac_debtors.`id` AS id,
CONCAT(dt_patientdetails.`Fname`," ",dt_patientdetails.`Lname`," ",dt_patientdetails.`surname`) AS patientName,
     COALESCE(ac_debtors.`balance`,0.0) AS balance,
     (select sum(c.credit) from credit_payment c where c.invoice_no=ac_debtors.invoice_no GROUP BY c.invoice_no) AS paid,
     ac_debtors.`billing_date` AS billing_date,
     ac_debtors.`cheque_no` AS cheque_no,
     ac_debtors.`debit` AS debit,
     (ac_debtors.`debit`- ac_debtors.`credit`) AS amount,
     ac_debtors.`bill_code` AS bill_code,
     ac_debtors.`invoice_no` AS invoice_no,
     ac_debtors.`patient_id` AS patient_id,
     ac_debtors.`payer` AS insuranceCode,
     ac_debtors.`scheme_code` AS schemeCode,
     ac_debtors.`ver_reason` AS remarks,
     dt_insurances.`insurance_name` AS insuranceName,
     dt_schemes.`scheme_name` AS schemeName,
     dt_schemes.`copay` AS dt_schemes_copay,
     dt_schemes.`discount` AS dt_schemes_discount
FROM
     `ac_debtors` ac_debtors LEFT JOIN `dt_patientdetails` dt_patientdetails ON ac_debtors.`patient_id` = dt_patientdetails.`patient_id`
     LEFT JOIN  dt_insurances ON ac_debtors.`payer` = dt_insurances.`insurance_code`
     LEFT JOIN `dt_schemes` dt_schemes ON ac_debtors.`scheme_code` = dt_schemes.`id`

WHERE ac_debtors.`billing_date` BETWEEN $P{from} AND $P{to} AND  ($P{insurance}="" OR dt_insurances.`insurance_code`=$P{insurance}) AND ac_debtors.`write_off`=1 AND ac_debtors.hospital_id=$P{hospitalId}
GROUP BY
     ac_debtors.hospital_id,
     ac_debtors.`payer`,
     ac_debtors.`invoice_no`]]>
	</queryString>
